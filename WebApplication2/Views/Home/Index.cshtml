@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1>
        Welcome to my webpp
    </h1>
    <button onclick="test()">
        Click me to get messages from server
    </button>
    <div id="dataContainer" style="display:flex;flex-direction:column;"></div>
</div>

<script>
    function test() {
        //alert("Hello world");
        $.ajax({
            url: "/Home/GetMessages",
            type: "GET",
            success: function (data) {
                //alert(data.message);
                console.log(data, "data");
                var dataContainerElement = document.getElementById("dataContainer");
                if (data.data.length > 0) {
                    var childElement = `<ul>${data.data.map(item => `<li>${item}</li>`).join('')}</ul>`;
                }
            },
            error: function (error) {
                alert("Error");
            }
        });
    }
</script>
